{
  "name": "vaah",
  "debug": true,
  "version": "1.0.0",
  "description": "VaahCLI - Command Line Tool (CLI) for Vaah Products",
  "author": "webreinvent",
  "bin": {
    "vaah": "./bin/run"
  },
  "homepage": "https://github.com/webreinvent/vaahcli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "webreinvent/vaahcli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@inquirer/prompts": "^4.2.0",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^3.8.3",
    "axios": "^1.5.1",
    "path": "^0.12.7",
    "ora": "^7.0.1",
    "listr2": "^7.0.1",
    "fs": "^0.0.1-security",
    "inquirer": "^9.2.11",
    "ejs": "^3.1.9",
    "semver": "^7.5.4",
    "os": "^0.1.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.5.6",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.55",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3.17.1",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"

  },
  "oclif": {
    "bin": "vaah",
    "dirname": "vaah",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "cms:install": "./bin/dev cms:install"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/webreinvent/vaahcli/issues",
  "keywords": [
    "vaah", "vaahcli", "vaahcms", "vaahstore", "vaahsaas"
  ],
  "types": "dist/index.d.ts"
}
